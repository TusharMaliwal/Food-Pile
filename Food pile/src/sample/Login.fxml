<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="299.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller_login">
   <Label fx:id="headLabel" layoutX="232.0" prefHeight="71.0" prefWidth="136.0" text="Login">
      <font>
         <Font name="System Bold Italic" size="49.0"/>
      </font>
   </Label>
   <Label fx:id="usernameLabel" layoutX="168.0" layoutY="98.0" prefHeight="22.0" prefWidth="63.0" text="Username:">
      <font>
         <Font name="System Italic" size="12.0"/>
      </font>
   </Label>
   <Label fx:id="passwordLabel" layoutX="168.0" layoutY="134.0" prefHeight="18.0" prefWidth="63.0" text="Password:">
      <font>
         <Font name="System Italic" size="12.0"/>
      </font>
   </Label>
   <TextField fx:id="usernameTextField" layoutX="226.0" layoutY="96.0" promptText="Enter Username"/>
   <Button fx:id="LoginButton" layoutX="268.0" layoutY="174.0" mnemonicParsing="false" onAction="#loginListener"
           prefHeight="26.0" prefWidth="63.0" text="Login" textFill="#1d1e1d">
      <font>
         <Font name="System Bold Italic" size="12.0"/>
      </font>
   </Button>
   <Label fx:id="instructionLabel" layoutX="168.0" layoutY="230.0" text="New User?"/>
   <Button fx:id="SignupButton" layoutX="232.0" layoutY="226.0" mnemonicParsing="false" onAction="#signupListener"
           text="SignUp">
      <font>
         <Font name="System Bold Italic" size="12.0"/>
      </font>
   </Button>
   <PasswordField fx:id="passwordField" layoutX="226.0" layoutY="131.0" promptText="Enter Password"/>
</AnchorPane>
