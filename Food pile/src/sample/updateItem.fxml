<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller_updateitem">
      <Label fx:id="headLabel" layoutX="60.0" layoutY="40.0" text="To update items enter the following details:-">
         <font>
            <Font name="System Bold Italic" size="23.0" />
         </font>
      </Label>
      <Label fx:id="usernameLabel" layoutX="176.0" layoutY="117.0" text="Username:">
         <font>
            <Font name="System Italic" size="15.0" />
         </font>
      </Label>
      <Label fx:id="productidLabel" layoutX="176.0" layoutY="161.0" text="Product Id:">
         <font>
            <Font name="System Italic" size="15.0" />
         </font>
      </Label>
      <Label fx:id="quantityLabel" layoutX="180.0" layoutY="200.0" text="Quantity:">
         <font>
            <Font name="System Italic" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="usernameTextField" layoutX="266.0" layoutY="115.0" promptText="Enter Username" />
      <TextField fx:id="productidTextField" layoutX="266.0" layoutY="159.0" promptText="Enter Product Id" />
      <TextField fx:id="quantityTextField" layoutX="266.0" layoutY="198.0" promptText="Enter Quantity" />
      <Button fx:id="submitButton" layoutX="264.0" layoutY="260.0" mnemonicParsing="false" onAction="#submitListener" prefHeight="34.0" prefWidth="109.0" text="Submit">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
</AnchorPane>
