<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller_search">
   <children>
      <Label fx:id="headLabel" layoutX="236.0" layoutY="25.0" text="Search">
         <font>
            <Font name="System Bold Italic" size="41.0" />
         </font>
      </Label>
      <TextField fx:id="nameTextField" layoutX="198.0" layoutY="168.0" prefHeight="32.0" prefWidth="177.0" promptText="Enter Name" />
      <Button fx:id="nameButton" layoutX="219.0" layoutY="214.0" mnemonicParsing="false" onAction="#nameListener" text="Search by Name">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font></Button>
      <TextField fx:id="categoryTextField" layoutX="197.0" layoutY="269.0" prefHeight="32.0" prefWidth="178.0" promptText="Enter Category" />
      <Button fx:id="categoryButton" layoutX="209.0" layoutY="317.0" mnemonicParsing="false" onAction="#categoryListener" text="Search by Category">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font></Button>
      <TextField fx:id="usernameTextField" layoutX="186.0" layoutY="106.0" prefHeight="32.0" prefWidth="198.0" promptText="Enter Username" />
   </children>
</AnchorPane>
